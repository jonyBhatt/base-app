{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@Navigation/*": ["src/navigation/*"],
      "@Components/*": ["src/component/*"],
      "@Screens/*": ["src/screens/*"],
      "@Stores/*": ["src/stores/*"],
      "@Assets/*": ["assets/*"]
    }
  }
}
